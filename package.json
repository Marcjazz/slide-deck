{
  "name": "kdmarc-workspace",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:slide-deck": "yarn workspace @kuidjamarco/slide-deck build",
    "dev:slide-deck": "yarn workspace @kuidjamarco/slide-deck dev",
